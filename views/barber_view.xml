<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_pos_barber" model="ir.actions.act_window">
        <field name="name">Barber</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">kanban,tree,form</field>
        <field name="context">{"search_default_barber_filter": 1, "default_partner_status": "barber"}</field>
        <field name="domain"></field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Input New Barber!
            </p>
        </field>
    </record>

    <menuitem id="menu_pos_barber" 
        name="Barber"
        action="action_pos_barber"
        parent="point_of_sale.menu_point_of_sale"
        sequence="10"/>


</odoo>
